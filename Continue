$n = 0
while ($n -lt 11){ $n++
    if ($n -eq 5) {continue}
    Write-Host $n
}

$par = 0
while ($par -le 20){
    $par++
    if ($par % 2 -eq 0){continue}
    else{" $par O nÃºmero Ã© impar"}
}

$par = 0
while ($par -le 20){
    $par++
    if ($par % 2 -eq 0){break}
    else{" $par O nÃºmero Ã© impar"}
}

$s = 0 
while ($s -le 1000){
    if($s -eq 11){break}
    Write-Host $s
    $s++
}  
